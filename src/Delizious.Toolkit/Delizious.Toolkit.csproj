    <Authors>Oliver Zick</Authors>
    <Copyright>Copyright Â© 2020 Oliver Zick. All rights reserved.</Copyright>
    <RepositoryUrl>https://github.com/oliverzick/Delizious-Toolkit</RepositoryUrl>
    <RootNamespace>Delizious</RootNamespace>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <WarningLevel>0</WarningLevel>
    <OutputPath>bin\Debug</OutputPath>
